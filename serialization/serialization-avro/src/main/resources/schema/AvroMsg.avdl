@namespace ( "io.mercury.serialization.avro.msg" )
protocol UselessProtocol {
	
	enum ContentType {
		INT,
		LONG,
		DOUBLE,
		STRING,
		OBJECT,
		LIST,
		MAP
	}

	record Envelope {
		int code;
		int version;
		ContentType contentType;
	}

	record AvroBinaryMsg {
		Envelope envelope;
		long sequence;
		long epoch;
		bytes content;
	}

	record AvroTextMsg {
		Envelope envelope;
		long sequence;
		long epoch;
		string content;
	}

}
